.row{style:"margin-bottom:20px"}
  .col-md-6
    #timing{:style => "min-width: 310px; height: 400px; max-width: 600px; margin: 0 auto"}

  .col-md-6
    .tiles.black.m-b-10.clearfix
      .col-md-6.col-sm-12.no-padding
        .col-md-12.p-t-35.p-r-20.p-b-30.col-sm-12
          .col-md-12.col-sm-6
            .row.b-b.b-grey.p-b-10 
              Ranking Most RT By
              .col-md-12.col-sm-6.col-xs-6
                %p.bold.small-text=@actor.name 
                %h3.bold.text-success is RT by:
            -@keys.first(5).each do |key|          
              .row.m-t-15
                .col-md-6.col-sm-6.col-xs-6
                  =link_to "http://twitter.com/#{key.dup}",target: 'blank' do
                    %p.bold.text-white=key.dup
                .col-md-6.col-sm-6.col-xs-6
                  %p.text-warning
                    =@rt_by[key].to_s
                    RT
                    %i.fa.fa-sort-asc.fa-lg.text-error{:style => "vertical-align: super;"}
      .col-md-6.col-sm-12.no-padding
        .col-md-12.p-t-35.p-r-20.p-b-30.col-sm-12
          .col-md-12.col-sm-6
            .row.b-b.b-grey.p-b-10 
              Ranking Most Replies to
              .col-md-12.col-sm-6.col-xs-6
                %p.bold.small-text=@actor.name
                %h3.bold.text-success talks to:
            -@my_keys.first(5).each do |key|          
              .row.m-t-15
                .col-md-6.col-sm-6.col-xs-6
                  =link_to "http://twitter.com/#{key}", taget: 'blank' do
                    %p.bold.text-white=key
                .col-md-6.col-sm-6.col-xs-6
                  %p.text-warning
                    =@my_rt[key].to_s
                    Tweets
                    %i.fa.fa-sort-asc.fa-lg.text-error{:style => "vertical-align: super;"}
        

