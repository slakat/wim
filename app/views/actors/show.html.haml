.row
  .col-md-3
    .tiles.green.overflow-hidden.full-height{:style => "max-height:300px"}
      .overlayer.bottom-right.fullwidth
        .overlayer-wrapper
          .tiles.gradient-black.p-l-20.p-r-20.p-b-20.p-t-20
            .pull-right
              =link_to "http://twitter.com/#{@actor.screen_name[1..-1]}",id:'selected-actor-profile',class:"hashtags transparent" do
                .i.fa.fa-twitter
                Twitter
            .clearfix
      =image_tag '', class:"#{@actor.screen_name[1..-1]} sprite lazy hover-effect-img actor-show"
    .tiles.black
      .tiles-body.txt-18
        .row
          %h4.text-white.text-center
            .i.fa.fa-user
            %span#selected-actor-name
              =@actor.name
          %h4.text-white.text-center

    .row
      .grid{:style => "margin-top:30px;margin-left:10px"}
        = link_to 'Download All RT', retweets_path(format: :xlsx, id:@actor.id), class: "btn btn-primary",:style => "width:300px"
        %br
        =# link_to 'Download CSV list', mentions_path(format: :csv), class:"btn btn-info",:style => "width:300px"
        = link_to 'Download Most RT', mostrt_path(format: :xlsx,actor_id:@actor.id), class:"btn btn-info",:style => "width:300px"

  .col-md-9
    .row.tiles-container
      .col-md-4.col-sm-4.tiles.dark-blue{:style => "height:130px"}
        .overlayer.top-right
          .p-r-15.p-t-15
            %i.fa.fa-bullhorn.text-white.fa-4x
        .overlayer.bottom-left
          .p-l-15.p-b-15
            %h2.text-white=number_with_delimiter(@total_tweets, :delimiter => ',')
            %h4.text-white Total Tweets
            %span.text-white.mini-description
              ="#{@original}% are original messages"
      .col-md-4.col-sm-4.tiles.light-blue{:style => "height:130px"}
        .overlayer.top-right
          .p-r-15.p-t-15
            %i.fa.fa-reply.text-white.fa-4x
        .overlayer.bottom-left
          .p-l-15.p-b-15
            %h2.text-white=number_with_delimiter(@total_mentions, :delimiter => ',')
            %h4.text-white Total Mentions
            %span.text-white.mini-description 
              ="#{@mentions}% are reply tweets"
      .col-md-4.col-sm-4.tiles.light-red{:style => "height:130px"}
        .overlayer.top-right
          .p-r-15.p-t-15
            %i.fa.fa-retweet.text-white.fa-4x
        .overlayer.bottom-left
          .p-l-15.p-b-15
            %h2.text-white=number_with_delimiter(@total_rt, :delimiter => ',')
            %h4.text-white Total Retweets
            %span.text-white.mini-description 
              ="With #{@actor.ranking_tweets.first.retweets} retweets for the hottest message"

    .col-md-12.hidden-xlg
      .row.tiles-container.m-b-10.hidden-xlg
        .col-md-12
          .m-l-10
            .tiles.grey.p-t-15.p-b-15.p-l-25
              %h5.text-black.semi-bold 
                MOST POPULAR TWEETS
                |
                =link_to 'See Full Ranking', actor_retweets_path(@actor.id), class:"text-success bold"
            .tiles.white
              -@actor.ranking_tweets.first(3).each do |r|
                .p-t-20.p-b-15.b-b.b-grey
                  .post.overlap-left-10
                    .user-profile-pic-wrapper
                      .user-profile-pic-2x.white-border
                        %img{:alt => "", class:"#{@actor.screen_name[1..-1]} sprite", :zoom=> ".15",:height => "45", :width => "45"}/
                    .info-wrapper.small-width.inline
                      .info.text-black
                        %p
                          =r.text
                        %p.muted.small-text
                          =r.tweet_date.to_date
                      .clearfix
                    .inline.pull-right
                      .tiles.white.text-success.bold.p-t-5.p-l-5.p-b-5.p-r-5.inline
                        %i.fa.fa-heart-o.fa-lg
                        =r.retweets
                        RT
                      .tiles.white.text-success.bold.p-t-5.p-l-5.p-b-5.p-r-5.inline
                        =link_to r.permanent_link, class:"btn btn-warning" do 
                          %i.fa.fa-info.fa-lg
                    .clearfix
%br
